{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-shadow": 0,
    "no-param-reassign": 0,
    "eol-last": 0,
    "arrow-parens": [2, "always"],
    "import/extensions": [0, "ignorePackages"],
    "no-underscore-dangle": 0
  }
}
